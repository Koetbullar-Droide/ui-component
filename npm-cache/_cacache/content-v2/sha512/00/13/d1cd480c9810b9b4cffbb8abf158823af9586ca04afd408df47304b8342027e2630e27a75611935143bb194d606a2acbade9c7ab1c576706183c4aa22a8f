      "dist" : {
        "tarball" : "https://bin.swisscom.com/artifactory/api/npm/swisscom-npm-virtual/lru-cache/-/lru-cache-7.5.2.tgz",
        "shasum" : "f7da8d0a1906bacb397e0747796d53b08441d877",
        "integrity" : "sha512-Vd4QEKEWXQeV20F2GffIwDYYDcplgpcozPDzv59jD8+AUgmVYCATaFrZu4efkCeYfMeJsOz6ZnkdWPssI4jRyQ=="
