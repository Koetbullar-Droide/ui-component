        "tarball" : "https://bin.swisscom.com/artifactory/api/npm/swisscom-npm-virtual/lru-cache/-/lru-cache-4.1.5.tgz",
        "shasum" : "8bbe50ea85bed59bc9e33dcab8235ee9bcf443cd",
